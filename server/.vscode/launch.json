   "version": "0.2.0",
   "configurations": [
      {
         "request": "launch",
         "name": "Launch Main",
         "type": "node",
         "program": "${workspaceFolder}/src/main.ts",
         "cwd": "${workspaceFolder}",
         "runtimeExecutable": "/home/shaw/.deno/bin/deno",
         "runtimeArgs": [
            "run",
            "--inspect-wait",
            "--allow-all",
            "--env"
         ],
         "attachSimplePort": 9229
      },
      {
         "request": "launch",
         "name": "Launch Oxford Web",
         "type": "node",
         "program": "${workspaceFolder}/oxford-web.ts",
         "args": [
            "bump into"
         ],
         "cwd": "${workspaceFolder}",
         "runtimeExecutable": "/home/shaw/.deno/bin/deno",
         "runtimeArgs": [
            "run",
            "--inspect-wait",
            "--allow-all",
            "--env"
         ],
         "attachSimplePort": 9229
      }
   ]
}